package com.company;

import java.util.Scanner;

public class PercentageOfCbse {
    public static void main(String[] args) {

        Scanner s= new Scanner(System.in);

        System.out.print("marks of subject1:");
        float m1=s.nextFloat();
        System.out.print("marks of subject2:");
        float m2=s.nextFloat();
        System.out.print("marks of subject3:");
        float m3=s.nextFloat();
        System.out.print("marks of subject4:");
        float m4=s.nextFloat();
        System.out.print("marks of subject5:");
        float m5=s.nextFloat();
        float per=((m1+m2+m3+m4+m5)/500)*100;
        System.out.println(" ");
    }
}
